<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="5" id="com.skyexplorer.moneycalc" version="0.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BarcodeScanner">
        <param name="browser-package" value="BarcodeScanner" />
    </feature>
    <name>Payment Per Person</name>
    <description>
        Payment Per Person
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
       Sky Explorer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <config-file mode="delete" parent="/manifest" platform="android">
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    </config-file>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="fullscreen" value="true" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="permissions" value="none" />
</widget>
